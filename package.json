{
  "name": "zofe",
  "description": "ZOFE",
  "version": "3.0.0",
  "author": "Daniel Filho",
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "engines": {
    "node": ">=12.16.3"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "eslint . --fix",
    "test": "eslint ."
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "plugin:react/recommended",
      "plugin:prettier/recommended",
      "prettier/react"
    ],
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "rules": {
      "react/react-in-jsx-scope": 0,
      "react/prop-types": 0,
      "eol-last": 2
    }
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "disqus-react": "^1.0.8",
    "dotenv": "^8.2.0",
    "gatsby": "^2.22.15",
    "gatsby-cli": "^2.12.40",
    "gatsby-plugin-feed": "^2.5.3",
    "gatsby-plugin-manifest": "^2.4.9",
    "gatsby-plugin-offline": "^3.2.7",
    "gatsby-plugin-react-helmet": "^3.3.2",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-typography": "^2.5.2",
    "gatsby-transformer-remark": "^2.8.13",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-player": "^1.11.1",
    "react-typography": "^0.16.19",
    "typography": "^0.16.19",
    "typography-theme-ocean-beach": "^0.16.19"
  },
  "devDependencies": {
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "gatsby-plugin-react-axe": "^0.4.1",
    "gatsby-source-contentful": "^2.3.12",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
