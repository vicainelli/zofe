{
  "name": "zofe",
  "description": "ZOFE",
  "version": "3.0.0",
  "author": "Daniel Filho",
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "engines": {
    "node": ">=12.18.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "eslint . --fix",
    "test": "eslint ."
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "plugin:react/recommended",
      "plugin:prettier/recommended",
      "prettier/react"
    ],
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "rules": {
      "react/react-in-jsx-scope": 0,
      "react/prop-types": 0,
      "eol-last": 2
    }
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "disqus-react": "^1.0.9",
    "dotenv": "^8.2.0",
    "gatsby": "^2.23.11",
    "gatsby-cli": "^2.12.51",
    "gatsby-plugin-feed": "^2.5.7",
    "gatsby-plugin-manifest": "^2.4.14",
    "gatsby-plugin-offline": "^3.2.13",
    "gatsby-plugin-react-helmet": "^3.3.6",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-typography": "^2.5.6",
    "gatsby-transformer-remark": "^2.8.19",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-player": "^2.3.1",
    "react-typography": "^0.16.19",
    "typography": "^0.16.19",
    "typography-theme-ocean-beach": "^0.16.19"
  },
  "devDependencies": {
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "gatsby-plugin-react-axe": "^0.4.1",
    "gatsby-source-contentful": "^2.3.18",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
